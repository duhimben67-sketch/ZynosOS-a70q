<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!-- Remotes -->
    <remote name="lineage" fetch="https://github.com/LineageOS/" />
    <remote name="muppets" fetch="https://github.com/TheMuppets/" />

    <default revision="lineage-21.0" remote="lineage" sync-j="4" />

    <!-- Device Trees -->
    <project name="android_device_samsung_a70q" path="device/samsung/a70q" remote="lineage" />
    <project name="android_device_samsung_sm6150-common" path="device/samsung/sm6150-common" remote="lineage" />
    <project name="android_device_samsung_common" path="device/samsung/common" remote="lineage" />

    <!-- Kernel -->
    <project name="android_kernel_samsung_sm6150" path="kernel/samsung/sm6150" remote="lineage" />

    <!-- Vendor Blobs -->
    <project name="proprietary_vendor_samsung" path="vendor/samsung/sm6150-common" remote="muppets" />
    <project name="proprietary_vendor_samsung_a70q" path="vendor/samsung/a70q" remote="muppets" />
    <project name="proprietary_vendor_qcom_sm6150" path="vendor/qcom/sm6150" remote="muppets" />

</manifest>
